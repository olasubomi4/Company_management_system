@model IEnumerable<ObaGroupModel.Document>  
@{
    ViewBag.Title = "title";
}
<h2>@ViewBag.Title</h2>
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Document List </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Document" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus"></i>  Create New Document
            </a>
        </div>
    </div>
</div>
<table  id="tblData" class="table table-bordered table-striped" style="width: 100%">
    <thead>
    <tr>
        <th>Name</th>
        <th>Type</th>
        <th>CreationDate</th>
        <th>Staff</th>
        <th></th>
        <th></th>
    </tr>
    </thead>
</table>

@section Scripts
{
    <script src="~/js/document.js">
    
    </script>
}

